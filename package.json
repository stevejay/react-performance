        "good-fences": "^0.9.0",
        "npm-check": "^5.9.0",
        "stylelint": "^12.0.0",
        "stylelint-config-recommended": "^3.0.0"
    },
    "babelMacros": {
        "styledComponents": {}
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run lint"
        }
    }
